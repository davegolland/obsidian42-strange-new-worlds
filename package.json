		"@codemirror/view": "^6.35.0",
		"@types/node": "^22.10.1",
		"builtin-modules": "4.0.0",
		"esbuild": "0.24.0",
		"obsidian": "^1.7.2",
		"obsidian-typings": "^2.3.4",
		"ts-prune": "^0.10.3",
		"tslib": "^2.8.1",
